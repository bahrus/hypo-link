{
    "version": 1,
    "tags": [
        {
            "name": "hypo-link",
            "description": "Web component wrapper around the anchorme library.",
            "selfResolvingModulePath": "https://unpkg.com/hypo-link@0.0.15/hypo-link.js?module",
            "attributes": [
                {
                    "name": "raw-content",
                    "description": "String to hyperlink.  Gets set from innerText initially."
                },
                {
                    "name": "truncate",
                    "description": "Trucate the link text"
                },
                {
                    "name": "default-protocol",
                    "description": "If the link came without protocol, like www.google.com then anchorme will add the https:// by default. However you can set your own default protocol."
                },
                {
                    "name": "exclude-emails",
                    "description": "Exclude email links"
                },
                {
                    "name": "exclude-urls",
                    "description": "Exclude url links"
                },
                {
                    "name": "exclude-ips",
                    "description": "Exclude ips links"
                },
                {
                    "name": "exclude-files",
                    "description": "Exclude file links"
                }
            ],
            "properties": [
                {
                    "name": "rawContent",
                    "description": "String to hyperlink.  Gets set from innerText initially.",
                    "type": "string"
                },
                {
                    "name": "truncate",
                    "description": "Trucate the link text",
                    "type": "boolean"
                },
                {
                    "name": "defaultProtocol",
                    "description": "If the link came without protocol, like www.google.com then anchorme will add the https:// by default. However you can set your own default protocol.",
                    "type": "string"
                },
                {
                    "name": "excludeEmails",
                    "description": "Exclude email links",
                    "type": "boolean"
                },
                {
                    "name": "excludeUrls",
                    "description": "Exclude url links",
                    "type": "boolean"
                },
                {
                    "name": "excludeIps",
                    "description": "Exclude ips links",
                    "type": "boolean"
                },
                {
                    "name": "excludeFiles",
                    "description": "Exclude file links",
                    "type": "boolean"
                }
            ]
        }
    ]
}